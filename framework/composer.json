{
    "name": "legobuilder/framework",
    "autoload": {
        "psr-4": {
            "Legobuilder\\Framework\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Thomas DELAPORTE",
            "email": "thomas.d@arpa3.fr"
        }
    ],
    "require": {
        "webonyx/graphql-php": "^15.5",
        "symfony/options-resolver": "^6.3"
    },
    "require-dev": {
        "phpstan/phpstan": "1.11.x-dev",
        "phpunit/phpunit": "9.6.x-dev"
    },
    "scripts": {
        "phpstan:analyse": "phpstan analyse src --level 5",
        "phpcs:analyse": "phpcs src",
        "phpcs:fix": "phpcbf src",
        "test": "phpunit"
    }
}
